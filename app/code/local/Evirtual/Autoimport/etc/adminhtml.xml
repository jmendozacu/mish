		</resources>
	</acl>
	<menu>
		<autoimport translate="title" module="autoimport">
			<title>Autoimport</title>
			<sort_order>85</sort_order>
			<depends>
				<module>Evirtual_Autoimport</module>
		 	</depends>
		 	<children>
				<entry translate="title" module="autoimport">
					<title>Entries</title>
					<action>adminhtml/autoimport_entry</action>
					<sort_order>0</sort_order>
				</entry>
				<activiy translate="title" module="autoimport">
					<title>Activites</title>
					<action>adminhtml/autoimport_activiy</action>
					<sort_order>10</sort_order>
				</activiy>
			</children>
		</autoimport>
	</menu>
